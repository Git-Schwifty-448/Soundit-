    <div class="col s10 offset-s1 content_card z-depth-1">
        <div class="content_title">
            All Users
        </div>

        <div class="content_body">

          <ul class="users">
            <%= render @users %>
          </ul>

          <%= will_paginate %>
        </div>
      </div>
  </div>