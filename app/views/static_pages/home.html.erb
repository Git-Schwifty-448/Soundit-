

        <div class="row row_margin">

            <div class="col l7 offset-l1 m12 s12 content_card z-depth-1 ">
                <div class="content_title hide-on-med-and-down">
                    What's Hot?
                </div>
                <div class="content_title_mobile hide-on-large-only">
                    What's Hot?
                </div>
                <ul class="feed_listing">
                    <%= render @feed %>
                </ul>
            </div>

            <div class="col s4">
                <div class="row hide-on-med-and-down">
                    <div class="col s10 offset-s1 content_card_small z-depth-1">
                        <div class="content_sub_title">
                            Communities
                        </div>
                        Information, including a genre listing will eventually go here.
                        <br><br><br><br><br><br><br><br><br>
                    </div>
                </div>

                <% if logged_in? %>
                    <div class="row">
                        <div class="col s10 offset-s1 content_card_small z-depth-1 hide-on-med-and-down">
                            <div class="content_sub_title">
                                My Submissions (<%= current_user.microposts.count %>)
                            </div>

                            <% for micropost in @posts %>
                                (<%= micropost.cached_votes_score %> pts) <%= link_to micropost.title, micropost %> by <%= micropost.artist %><br>
                            <% end %>
                        </div>
                    </div>

                
                    <div class="row">
                        <div class="col s10 offset-s1 content_card_small z-depth-1 hide-on-med-and-down">
                            <div class="content_sub_title">
                                My Upvoted Songs
                            </div>
                            <% @current = current_user.votes.up.for_type(Micropost) %>
                            <% for micropost in @current %>
                                <% @find = Micropost.find(micropost.votable_id) %>
                                (<%= @find.cached_votes_score %> pts) <%= link_to @find.title, @find %> by <%= @find.artist %><br>
                                <%# <%= render '/shared/micropost_skinny', collection: micropost %>
                            <% end %>
                        </div>
                    </div>
                <% end %>
            </div>
        </div>

        <div class="row hide-on-small-only">
                <div class="col s10 push-s1 content_card z-depth-1 hide-on-med-and-down">
                    <div class="content_title">
                        Community Feature
                    </div>
                    <div>
                    Community Feature<br><br><br><br>
                    </div>
                </div>
            </div>
        </div>



