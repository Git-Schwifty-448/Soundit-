        <div class="row">

            <div class="col l8  m12 s12  ">
                <div class="row">
                    <div class="col s12">
                        <div class="content_title">
                            Trending
                        </div>
                        <div class="row">
                            <div class="col s12">
                                <div class="row">
                                    <% @count = 0 %>
                                    <% for post in @community_listings %>

                                        <div class="col s4"> 
                                            <span class="_title"><%= link_to @community_names[@count].first.title, @community_names[@count].first %></span>
                                            <div class="row">
                                                <div class="col s12 content_card z-depth-1">
                                                    <table class="bordered highlight ">
                                                        <% for each in post %>
                                                            <tr>
                                                                <td class="fp_song_title">

                                                                    <% if logged_in? %>
                                                                        <% if current_user.voted_up_on? each %>
                                                                            <span class="_upvoted_trend"> <%= link_to "∆", micropost_like_path(each), method: :put %></span>
                                                                        <% else %>
                                                                            <span class="_nonvoted_trend"> <%= link_to "∆", micropost_like_path(each), method: :put %></span>
                                                                        <% end %>
                                                                    <% else %>
                                                                        <span class="_nonvoted_trend"> <%= link_to "∆", micropost_like_path(each), method: :put %></span>
                                                                    <% end %>



                                                                    <span class="_score_trend"><%= each.cached_votes_score %></span>


                                                                    <% if logged_in? %>
                                                                        <% if current_user.voted_down_on? each %>
                                                                            <span class="_downvoted_trend"> <%= link_to "∇", micropost_dislike_path(each), method: :put %> </span>
                                                                        <% else %>
                                                                            <span class="_nonvoted_trend"> <%= link_to "∇", micropost_dislike_path(each), method: :put %></span>
                                                                        <% end %>
                                                                    <% else %>
                                                                        <span class="_nonvoted_trend"> <%= link_to "∇", micropost_dislike_path(each), method: :put %></span>
                                                                    <% end %>
                                                                    
                                                                    <%= each.title %> by <%= each.artist %>
                                                                </td>
                                                            </tr>
                                                        <% end %>

                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                    <% @count = @count + 1 %>
                                    <% end %>

                                </div>
                            </div>
                        </div>
                    </div>

                </div>
                <div class="row">
                    <div class="col s12">
                        <div class="content_title_small_margin">
                            Trending Tracks
                        </div>
                        <ul class="feed_listing">
                            <%= render @feed %>
                            <%= will_paginate @feed %>
                        </ul>
                    </div>
                </div>
            </div>

            <div class="col s4">
                <div class="row hide-on-med-and-down">
                    <div class="col s10 offset-s1 content_card_small z-depth-1">
                        <div class="content_sub_title">
                            Communities
                        </div>
                        Information, including a genre listing will eventually go here.
                        <br><br><br><br><br><br><br><br><br>
                    </div>
                </div>

                <% if logged_in? %>
                    <div class="row">
                        <div class="col s10 offset-s1 content_card_small z-depth-1 hide-on-med-and-down">
                            <div class="content_sub_title">
                                My Submissions (<%= current_user.microposts.count %>)
                            </div>

                            <% for micropost in @posts %>
                                (<%= micropost.cached_votes_score %> pts) <%= link_to micropost.title, micropost %> by <%= micropost.artist %><br>
                            <% end %>
                            
                        </div>
                    </div>

                
                    <div class="row">
                        <div class="col s10 offset-s1 content_card_small z-depth-1 hide-on-med-and-down">
                            <div class="content_sub_title">
                                My Upvoted Songs
                            </div>
                            <% @current = current_user.votes.up.for_type(Micropost) %>
                            <% for micropost in @current %>
                                <% @find = Micropost.find(micropost.votable_id) %>
                                (<%= @find.cached_votes_score %> pts) <%= link_to @find.title, @find %> by <%= @find.artist %><br>
                                <%# <%= render '/shared/micropost_skinny', collection: micropost %>
                            <% end %>
                            
                        </div>
                    </div>
                <% end %>
            </div>
        </div>



